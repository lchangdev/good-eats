<h1>Submit new restaurant</h1>
<br>
<%= form_for @restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, required: true %><br>
  <br>
  <%= f.label :address %>
  <%= f.text_field :address, required: true %><br>
  <br>
  <%= f.label :city %>
  <%= f.text_field :city, required: true %><br>
  <br>
  <%= f.label :state %>
  <%= f.text_field :state, required: true %><br>
  <br>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code, required: true %><br>
  <br>
  <%= f.label :category %>
  <%= f.text_field :category %><br>
  <br>
  <%= f.label :description %>
  <%= f.text_area :description %><br>
  <br>
  <%= f.submit 'submit' %>
<% end %>
<br>
<%= link_to 'back', restaurants_path %>
